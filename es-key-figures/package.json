{
  "name": "es-key-figures",
  "version": "0.1.0",
  "bin": {
    "cloudfront-cdk": "bin/es-key-figure.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules --no-minify",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "parcel-watch": "parcel watch lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules",
    "watch-bundle": "parallelshell \"npm run watch\" \"npm run parcel-watch\""
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.56.0",
    "@types/jest": "^26.0.8",
    "@types/node": "14.0.27",
    "jest": "24.2.0-alpha.0",
    "ts-jest": "^26.1.4",
    "aws-cdk": "^1.56.0",
    "ts-node": "^8.10.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "0.5.1",
    "rimraf": "^3.0.2",
    "@types/sinon": "^9.0.4",
    "sinon": "^9.0.2",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-events-targets": "^1.81.0",
    "@aws-cdk/aws-kinesis": "^1.81.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.81.0",
    "@aws-cdk/aws-logs-destinations": "^1.81.0",
    "@aws-cdk/aws-rds": "^1.81.0",
    "@aws-cdk/aws-secretsmanager": "^1.81.0",
    "@aws-cdk/aws-ssm": "^1.81.0",
    "@aws-cdk/core": "^1.81.0",
    "@types/aws-lambda": "^8.10.68",
    "@types/mysql": "^2.15.15",
    "aws-lambda": "^1.0.6",
    "axios": "0.19.2",
    "mysql": "^2.18.1",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
