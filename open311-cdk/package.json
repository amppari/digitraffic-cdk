{
  "name": "open311-cdk",
  "version": "0.1.0",
  "bin": {
    "open311-cdk": "bin/open311-cdk.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda-*.ts --target=node --global handler -d lib --bundle-node-modules --no-source-maps",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.18.0",
    "@types/aws-lambda": "latest",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "aws-cdk": "^1.18.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "latest",
    "@aws-cdk/core": "^1.18.0",
    "digitraffic-lambda-postgres": "file:../common/postgres",
    "pg-native": "^3.0.0",
    "pg-promise": "latest",
    "source-map-support": "^0.5.16"
  }
}
