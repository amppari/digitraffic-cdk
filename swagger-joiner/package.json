{
  "name": "swagger-joiner",
  "version": "0.1.0",
  "bin": {
    "cloudfront-cdk": "bin/swagger-joiner-app.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules --no-source-maps --no-minify",
    "watch": "tsc -w",
    "test": "jest --runInBand --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.32.2",
    "@types/aws-lambda": "latest",
    "@types/jest": "^25.2.1",
    "@types/node": "13.11.1",
    "@types/sinon": "^9.0.0",
    "aws-cdk": "1.32.2",
    "jest": "^25.3.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.4.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typescript": "~3.8.3"
  },
  "dependencies": {
    "@aws-cdk/aws-events-targets": "1.32.2",
    "@aws-cdk/core": "1.32.2",
    "@aws-cdk/aws-s3": "1.32.2",
    "@aws-cdk/aws-lambda": "1.32.2",
    "@aws-cdk/aws-logs": "1.32.2",
    "@aws-cdk/aws-iam": "1.32.2",
    "@types/ramda": "^0.27.3",
    "aws-sdk": "2.656.0",
    "axios": "latest",
    "ramda": "^0.27.0",
    "source-map-support": "^0.5.16"
  },
  "externals": [
    "aws-sdk"
  ]
}
