{
  "name": "swagger-joiner",
  "version": "0.1.0",
  "bin": {
    "cloudfront-cdk": "bin/swagger-joiner-app.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules --no-source-maps --no-minify",
    "watch": "tsc -w",
    "test": "jest --runInBand --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.27.0",
    "@types/aws-lambda": "latest",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "@types/sinon": "^7.5.1",
    "aws-cdk": "1.27.0",
    "jest": "^24.9.0",
    "parcel": "^1.12.4",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-events-targets": "1.27.0",
    "@aws-cdk/core": "1.27.0",
    "@aws-cdk/aws-s3": "1.27.0",
    "@aws-cdk/aws-lambda": "1.27.0",
    "@aws-cdk/aws-logs": "1.27.0",
    "@aws-cdk/aws-iam": "1.27.0",
    "@types/ramda": "^0.26.43",
    "aws-sdk": "2.637.0",
    "axios": "latest",
    "ramda": "^0.27.0",
    "source-map-support": "^0.5.16"
  }
}
