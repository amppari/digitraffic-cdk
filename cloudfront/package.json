{
  "name": "cloudfront-cdk",
  "version": "0.1.0",
  "bin": {
    "cloudfront-cdk": "bin/cloudfront-app.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules --no-source-maps",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.31.0",
    "@types/aws-lambda": "8.10.46",
    "@types/node": "13.9.8",
    "@types/sinon": "7.5.2",
    "aws-cdk": "1.31.0",
    "parcel": "1.12.4",
    "rimraf": "3.0.2",
    "sinon": "9.0.1",
    "ts-node": "8.8.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.31.0",
    "@aws-cdk/aws-wafv2": "1.31.0",
    "@aws-cdk/core": "1.31.0",
    "@aws-cdk/aws-events-targets": "1.31.0"
  }
}
