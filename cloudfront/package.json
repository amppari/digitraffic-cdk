{
  "name": "cloudfront-cdk",
  "version": "0.1.0",
  "bin": {
    "cloudfront-cdk": "bin/cloudfront-app.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "parcel_build": "parcel build lib/lambda/**/*.js --target=node --global handler -d dist/lambda --bundle-node-modules --no-source-maps",
    "parcel_elasticsearch": "parcel build node_modules/@elastic/elasticsearch/api/api/bulk.js --target=node --global handler -d dist/lambda/api --bundle-node-modules --no-source-maps",
    "postbuild": "npm runScript parcel_build",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.56.0",
    "@types/aws-lambda": "8.10.59",
    "@types/node": "14.0.27",
    "@types/sinon": "9.0.4",
    "aws-cdk": "1.56.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "0.5.1",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.56.0",
    "@aws-cdk/aws-wafv2": "1.56.0",
    "@aws-cdk/aws-s3-notifications": "1.56.0",
    "@aws-cdk/aws-cloudfront": "1.56.0",
    "@aws-cdk/core": "1.56.0",
    "aws-sdk": "2.727.1",
    "source-map-support": "0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
