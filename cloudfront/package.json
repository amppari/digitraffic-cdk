{
  "name": "cloudfront-cdk",
  "version": "0.1.0",
  "bin": {
    "cloudfront-cdk": "bin/cloudfront-app.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/*.js lib/lambda/**/*.js --target=node --global handler -d dist/lambda --bundle-node-modules --no-source-maps",
    "parcel-watch": "parcel watch lib/lambda/*.js lib/lambda/**/*.js --target=node --global handler -d dist/lambda --bundle-node-modules --no-source-maps",
    "watch-bundle": "parallelshell \"npm run watch\" \"npm run parcel-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.82.0",
    "@types/aws-lambda": "8.10.64",
    "@types/node": "14.0.27",
    "@types/sinon": "9.0.8",
    "aws-cdk": "1.71.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "0.5.1",
    "rimraf": "3.0.2",
    "sinon": "9.2.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "aws-sdk": "2.783.0",
    "parallelshell": "^3.0.1"
  },
  "dependencies": {
    "@aws-cdk/aws-logs": "1.82.0",
    "@aws-cdk/aws-lambda": "1.82.0",
    "@aws-cdk/aws-lambda-event-sources": "1.82.0",
    "@aws-cdk/aws-sqs": "1.82.0",
    "@aws-cdk/aws-kinesis": "1.82.0",
    "@aws-cdk/aws-apigateway": "1.82.0",
    "@aws-cdk/aws-wafv2": "1.82.0",
    "@aws-cdk/aws-s3-notifications": "1.82.0",
    "@aws-cdk/aws-cloudfront": "1.82.0",
    "@aws-cdk/core": "1.82.0",
    "source-map-support": "0.5.19",
    "p-limit": "3.0.2"
  },
  "externals": [
    "aws-sdk"
  ]
}
