{
  "name": "open311-cdk",
  "version": "0.1.0",
  "bin": {
    "open311-cdk": "bin/open311-cdk.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules --no-source-maps",
    "watch": "tsc -w",
    "test": "jest --runInBand --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.32.2",
    "@types/aws-lambda": "8.10.48",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.1",
    "@types/sinon": "9.0.0",
    "@types/xml2js": "0.4.5",
    "aws-cdk": "1.32.2",
    "jest": "25.3.0",
    "parcel": "1.12.4",
    "querystring": "0.2.0",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.32.2",
    "@aws-cdk/core": "1.32.2",
    "digitraffic-lambda-postgres": "file:../common/postgres",
    "digitraffic-cdk-api": "file:../common/api",
    "pg-native": "3.0.0",
    "pg-promise": "latest",
    "source-map-support": "0.5.16",
    "@aws-cdk/aws-events-targets": "1.32.2",
    "axios": "latest"
  }
}
