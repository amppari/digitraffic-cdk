{
  "name": "open311-cdk",
  "version": "0.1.0",
  "bin": {
    "open311-cdk": "bin/open311-cdk.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules --no-source-maps",
    "watch": "tsc -w",
    "test": "jest --runInBand --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.55.0",
    "@types/aws-lambda": "8.10.59",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.26",
    "@types/sinon": "9.0.4",
    "@types/xml2js": "0.4.5",
    "aws-cdk": "1.55.0",
    "jest": "26.0.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.4.0",
    "querystring": "0.2.0",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "ts-jest": "26.1.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.55.0",
    "@aws-cdk/core": "1.55.0",
    "digitraffic-lambda-postgres": "file:../common/postgres",
    "digitraffic-cdk-api": "file:../common/api",
    "pg-native": "3.0.0",
    "pg-promise": "10.5.8",
    "source-map-support": "0.5.19",
    "@aws-cdk/aws-events-targets": "1.55.0",
    "axios": "0.19.2",
    "aws-sdk": "2.692.0"
  },
  "externals": [
    "aws-sdk"
  ]
}
