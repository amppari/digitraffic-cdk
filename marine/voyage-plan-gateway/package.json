{
  "name": "voyage-plan-gateway",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node12",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "test": "jest --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.83.0",
    "@types/aws-lambda": "^8.10.59",
    "@types/jest": "^26.0.8",
    "@types/jsts": "^0.17.5",
    "@types/node": "14.0.27",
    "@types/sinon": "^9.0.4",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "esbuild": "^0.8.36",
    "eslint": "^7.13.0",
    "jest": "^26.2.2",
    "jest-junit": "^12.0.0",
    "parallelshell": "3.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.83.0",
    "@aws-cdk/aws-certificatemanager": "1.83.0",
    "@aws-cdk/aws-ec2": "1.83.0",
    "@aws-cdk/aws-events-targets": "1.83.0",
    "@aws-cdk/aws-lambda": "1.83.0",
    "@aws-cdk/core": "1.83.0",
    "@types/openlayers": "^4.6.17",
    "asn1.js": "^4.8.0",
    "aws-sdk": "^2.861.0",
    "axios": "^0.21.1",
    "jsts": "^2.6.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "node-cache": "^5.1.2",
    "parse-link-header": "^1.0.1",
    "pkijs": "^2.1.94",
    "pvutils": "^1.0.17",
    "source-map-support": "^0.5.19",
    "wkx": "^0.5.0"
  },
  "externals": [
    "aws-sdk"
  ],
  "workspaces": {
    "nohoist": [
      "jsts",
      "@types/jsts",
      "@types/openlayers"
    ]
  }
}
