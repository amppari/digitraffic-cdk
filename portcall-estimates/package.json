    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.56.0",
    "@types/aws-lambda": "8.10.59",
    "@types/jest": "26.0.8",
    "@types/node": "14.0.27",
    "@types/sinon": "9.0.4",
    "@types/xml2js": "0.4.5",
    "jest": "26.2.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "0.5.1",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "ts-jest": "26.1.4",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "aws-sdk": "2.730.0"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.56.0",
    "@aws-cdk/aws-cloudwatch": "1.56.0",
