{
  "name": "maintenance-tracking",
  "version": "0.1.0",
  "bin": {
    "maintenance-tracking-app": "bin/maintenance-tracking-app.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules --no-minify",
    "watch": "tsc -w",
    "parcel-watch": "parcel watch lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules",
    "watch-bundle": "parallelshell \"npm run watch\" \"npm run parcel-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/sinon": "^9.0.8",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "aws-sdk": "^2.777.0",
    "eslint": "^7.13.0",
    "jest": "^26.6.0",
    "parallelshell": "3.0.1",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.83.0",
    "@aws-cdk/aws-cloudwatch": "1.83.0",
    "@aws-cdk/aws-cloudwatch-actions": "1.83.0",
    "@aws-cdk/aws-events-targets": "1.83.0",
    "@aws-cdk/aws-iam": "1.83.0",
    "@aws-cdk/aws-lambda-event-sources": "1.83.0",
    "@aws-cdk/aws-pinpoint": "1.83.0",
    "@aws-cdk/aws-s3": "1.83.0",
    "@aws-cdk/aws-sns": "1.83.0",
    "@aws-cdk/aws-sqs": "1.83.0",
    "@aws-cdk/core": "1.83.0",
    "@middy/core": "^1.2.0",
    "@middy/sqs-partial-batch-failure": "^1.2.0",
    "@types/ramda": "^0.27.27",
    "ajv-cli": "^3.3.0",
    "dtsgenerator": "^3.3.1",
    "moment": "^2.29.1",
    "pg-native": "3.0.0",
    "pg-promise": "10.8.6",
    "pg-query-stream": "4.0.0",
    "ramda": "^0.27.1",
    "source-map-support": "v0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
